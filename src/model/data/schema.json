        "PaginationMeta": {
            "type": "object",
            "required": [
                "page",
                "limit",
                "total",
                "hasMore"
            ],
            "properties": {
                "page": {
                    "type": "integer",
                    "description": "Current page number"
                },
                "limit": {
                    "type": "integer",
                    "description": "Items per page"
                },
                "total": {
                    "type": "integer",
                    "description": "Total items count"
                },
                "hasMore": {
                    "type": "boolean",
