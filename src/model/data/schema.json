{
    "definitions": {
        "HomeData": {
            "type": "object",
            "required": [
                "headline",
                "paragraph",
                "videoSource",
                "thumbnail"
            ],
            "properties": {
                "headline": {
                    "type": "string",
                    "description": "Main headline for home page"
                },
                "paragraph": {
                    "type": "string",
                    "description": "Descriptive paragraph for home page"
                },
                "videoSource": {
                    "type": "string",
                    "format": "uri",
                    "description": "URL to embedded video"
                },
                "thumbnail": {
                    "type": "string",
                    "format": "uri",
                    "description": "URL to video thumbnail image"
                }
            }
        },
        "JobListing": {
            "type": "object",
            "required": [
                "id",
                "title",
                "department",
                "location",
                "employment_type",
                "posted_date",
                "description",
                "responsibilities",
                "requirements",
                "benefits",
                "apply_link"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "description": "Unique job listing ID"
                },
                "title": {
                    "type": "string",
                    "description": "Job title"
                },
                "department": {
                    "type": "string",
                    "description": "Department name"
                },
                "location": {
                    "type": "string",
                    "description": "Job location"
                },
                "employment_type": {
                    "type": "string",
                    "enum": [
                        "Full-time",
                        "Part-time",
                        "Contract",
                        "Internship"
                    ],
                    "description": "Type of employment"
                },
                "posted_date": {
                    "type": "string",
                    "format": "date",
                    "pattern": "^\\d{4}-\\d{2}-\\d{2}$",
                    "description": "Date job was posted (YYYY-MM-DD)"
                },
                "description": {
                    "type": "string",
                    "description": "Job description"
                },
                "responsibilities": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "description": "List of job responsibilities"
                },
                "requirements": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "description": "List of job requirements"
                },
                "benefits": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "description": "List of job benefits"
                },
                "apply_link": {
                    "type": "string",
                    "format": "uri",
                    "description": "Link to apply for the job"
                }
            }
        },
        "CareerCTA": {
            "type": "object",
            "required": [
                "headline",
                "text",
                "submit_cv_link"
            ],
            "properties": {
                "headline": {
                    "type": "string",
                    "description": "CTA headline"
                },
                "text": {
                    "type": "string",
                    "description": "CTA description text"
                },
                "submit_cv_link": {
                    "type": "string",
                    "format": "uri",
                    "description": "Link to submit CV"
                }
            }
        },
        "CareerPage": {
            "type": "object",
            "required": [
                "headline",
                "subheadline",
                "job_listings",
                "cta_section"
            ],
            "properties": {
                "headline": {
                    "type": "string",
                    "description": "Page headline"
                },
                "subheadline": {
                    "type": "string",
                    "description": "Page subheadline"
                },
                "job_listings": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/JobListing"
                    },
                    "description": "List of job listings"
                },
                "cta_section": {
                    "$ref": "#/definitions/CareerCTA",
                    "description": "Call-to-action section"
                }
            }
        },
        "CareerData": {
            "type": "object",
            "required": [
                "career_page"
            ],
            "properties": {
                "career_page": {
                    "$ref": "#/definitions/CareerPage"
                }
            }
        },
        "NewsItem": {
            "type": "object",
