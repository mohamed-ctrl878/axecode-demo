                "headline": {
                    "type": "string",
                    "description": "Page headline"
                },
                "subheadline": {
                    "type": "string",
                    "description": "Page subheadline"
                }
            }
        },
        "UserProfile": {
            "type": "object",
            "required": [
                "id",
                "username",
                "email",
                "skills",
                "experience_level",
                "created_at"
            ],
            "properties": {
                "id": {
                    "type": "string",
                    "description": "User ID"
                },
                "username": {
                    "type": "string",
                    "description": "Username"
                },
