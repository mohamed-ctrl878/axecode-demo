        "FooterData": {
            "type": "object",
            "required": [
                "sections",
                "copyright"
            ],
            "properties": {
                "sections": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/FooterSection"
                    },
                    "description": "Footer sections"
                },
                "copyright": {
                    "type": "string",
                    "description": "Copyright text"
                },
                "socialLinks": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "required": [
                            "platform",
                            "url"
                        ],
                        "properties": {
                            "platform": {
                                "type": "string",
                                "description": "Social media platform name"
                            },
                            "url": {
                                "type": "string",
                                "format": "uri",
                                "description": "Social media profile URL"
                            }
                        }
                    },
                    "description": "Social media links"
                }
            }
        },
        "NavLink": {
            "type": "object",
            "required": [
                "label",
                "href"
            ],
            "properties": {
                "label": {
                    "type": "string",
                    "description": "Navigation link label"
                },
                "href": {
                    "type": "string",
                    "format": "uri",
                    "description": "Link URL"
                },
                "icon": {
                    "type": "string",
                    "description": "Icon name or path"
                }
            }
        },
        "NavData": {
            "type": "object",
            "required": [
                "brand",
                "links"
            ],
            "properties": {
                "brand": {
                    "type": "string",
                    "description": "Brand/company name"
                },
                "links": {
                    "type": "array",
                    "items": {
                        "oneOf": [
                            {
                                "$ref": "#/definitions/NavLink"
                            },
                            {
                                "type": "object",
                                "allOf": [
                                    {
                                        "$ref": "#/definitions/NavLink"
                                    },
                                    {
                                        "properties": {
                                            "submenu": {
                                                "type": "array",
                                                "items": {
                                                    "$ref": "#/definitions/NavLink"
                                                },
                                                "description": "Dropdown submenu items"
                                            }
                                        }
                                    }
                                ]
                            }
                        ]
                    },
                    "description": "Navigation links with optional submenus"
                }
            }
        },
        "ChatMessage": {
