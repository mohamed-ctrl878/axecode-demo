        "UserProgress": {
            "type": "object",
            "required": [
                "user_id",
                "courses_completed",
                "challenges_solved",
                "badges",
                "total_points",
                "level"
            ],
            "properties": {
                "user_id": {
                    "type": "string",
                    "description": "User ID"
                },
                "courses_completed": {
                    "type": "integer",
