                },
                "summary": {
                    "type": "string",
                    "description": "Brief summary of the news"
                },
                "content": {
                    "type": "string",
                    "description": "Full news content"
                },
                "link": {
                    "type": "string",
                    "format": "uri",
                    "description": "Link to full article"
