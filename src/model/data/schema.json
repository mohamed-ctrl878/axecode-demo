        "APIResponse": {
            "type": "object",
            "required": [
                "success",
                "data"
            ],
            "properties": {
                "success": {
                    "type": "boolean",
                    "description": "Request success status"
                },
                "data": {
                    "type": "object",
                    "description": "Response data"
                },
                "message": {
                    "type": "string",
                    "description": "Response message"
                },
                "pagination": {
                    "$ref": "#/definitions/PaginationMeta"
