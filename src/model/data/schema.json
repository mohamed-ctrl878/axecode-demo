                "conversation_id",
                "title",
                "participants_count",
                "messages",
                "created_at",
                "updated_at"
            ],
            "properties": {
                "conversation_id": {
                    "type": "string",
                    "description": "Unique conversation ID"
                },
                "title": {
                    "type": "string",
                    "description": "Conversation title"
                },
                "governorate": {
                    "type": "string",
