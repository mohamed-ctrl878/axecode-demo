                "subheadline",
                "features"
            ],
            "properties": {
                "headline": {
                    "type": "string",
                    "description": "MVP headline"
                },
                "subheadline": {
                    "type": "string",
                    "description": "MVP subheadline"
                },
                "features": {
                    "type": "array",
