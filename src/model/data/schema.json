                },
                "news_list": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/NewsItem"
                    },
                    "description": "List of news items"
                }
            }
        },
        "NewsData": {
            "type": "object",
            "required": [
                "news_page"
            ],
            "properties": {
                "news_page": {
                    "$ref": "#/definitions/NewsPage"
                }
            }
        },
        "FeatureItem": {
