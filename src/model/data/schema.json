        "ValidationError": {
            "type": "object",
            "required": [
                "field",
                "message"
            ],
            "properties": {
                "field": {
                    "type": "string",
                    "description": "Field name with error"
                },
                "message": {
                    "type": "string",
                    "description": "Error message"
                }
            }
        },
        "ValidationResult": {
            "type": "object",
            "required": [
                "isValid",
                "errors"
            ],
            "properties": {
                "isValid": {
                    "type": "boolean",
                    "description": "Validation result"
                },
                "errors": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/ValidationError"
                    },
                    "description": "List of validation errors"
                }
            }
        }
